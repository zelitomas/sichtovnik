{
  "name": "sichtovnik",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^2.0.3",
    "gh-pages": "^1.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "4.0.0",
    "cordova-android": "^8.1.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova": "^9.0.0"
  },
  "devdepencencies": {
    "cordova-plugin-whitelist": "1"
  },
  "scripts": {
    "predeploy": "REACT_APP_MODE='zelezarny' npm run build",
    "deploy": "gh-pages -d build",
    "prepare-cordova": "mkdir -p platforms www && cordova prepare",
    "build-android": "npm run prepare-cordova && npm run build && cordova build --release",
    "start": "react-scripts start",
    "build": "react-scripts build && cp -rT build www",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  },
  "homepage": "."
}